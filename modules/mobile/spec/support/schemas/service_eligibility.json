  ],
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id",
          "type",
          "attributes"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "attributes": {
            "type": "object",
            "required": [
              "request",
              "direct"
            ],
            "properties": {
              "request": {
                "type": "array"
              },
              "direct": {
                "type": "array"
              }
            }
          }
        }
      }
    }
  }
}